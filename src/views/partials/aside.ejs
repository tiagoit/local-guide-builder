<!-- cities widget -->
<div class="aside-widget">
    <div class="title">
        <h2>Cidades</h2>
    </div>
    <div class="cities-widget">
        <ul>
            <% cities.forEach(function(city) { %>
                <li class="<%= (locals.activeCity && activeCity == city.name) ? 'active' : '' %>">
                    <a href="/eventos/<%= city.code %>"><%= city.name %></a>
                </li>
            <% }); %>
        </ul>
    </div>

    <!-- <button class="primary-button d-block mx-auto mt-3">Filtrar</button> -->
</div>
<!-- /cities widget -->
    
